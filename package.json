{
  "name": "rfp-proposal-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "start": "npm-run-all --parallel start:backend preview:frontend",
    "start:backend": "npm --prefix backend run start",
    "preview:frontend": "npm --prefix frontend run preview",
    "install:all": "npm --prefix backend install && npm --prefix frontend install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}